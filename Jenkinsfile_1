pipeline{
	agent any
	stages{ 
		stage ('SCM Check Out'){
			steps {
				git url: 'https://github.com/walij/msgfw2.0_Demo.git'
			}
		}
		stage ('Run Smoke Test') { 
			steps {
				bat label: '', script: 'java -jar sampleCICDProcess.jar'
			}
		} 
		stage ('Publish Report') { 
			steps {
				publishHTML([allowMissing: false, alwaysLinkToLastBuild: true, keepAll: true, reportDir: 'reports', reportFiles: 'ExecutionReport.html#!', reportName: 'SmokeTestReport', reportTitles: 'Automation Test Execution Report'])
			}
		} 
	}
}
