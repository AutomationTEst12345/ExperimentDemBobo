pipeline{
	agent any
	stages{ 
		stage ('SCM Check Out'){
			steps {
				git url: 'https://github.com/walij/msgfw2.0_Demo.git'
			}
		}
		stage ('Run Smoke Test') { 
			steps {
				bat label: '', script: 'java -jar sampleCICDProcess.jar'
			}
		} 
		stage ('Publish Report') { 
			steps {
				publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: '\\Test Results\\', reportFiles: 'ExecutionReport.html#!', reportName: 'HTML Report', reportTitles: ''])
			}
		} 
	}
}
